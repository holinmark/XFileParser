                      | TOKEN_CSTRING
					  | TOKEN_NAME

array_data_type       : primitive_array_type

dimension_size        : TOKEN_INTEGER | TOKEN_NAME

dimension             : TOKEN_OBRACKET dimension_size TOKEN_CBRACKET

following_dimension_list : /* Empty */ | dimension following_dimension_list

dimension_list        : dimension following_dimension_list

