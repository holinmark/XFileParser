							template_option_info
                        TOKEN_CBRACKET
                      | template_members_list following_template_members_part

following_template_members_part : / * Empty */ | TOKEN_OBRACKET template_option_info TOKEN_CBRACKET

template_option_info  : ellipsis | template_option_list

template_members      : primitive_type following_primitive_type | array | template_reference

following_template_members_list : /* Empty */ | template_members following_template_members_list

template_members_list : template_members following_template_members_list

array                 : TOKEN_ARRAY array_data_type TOKEN_NAME dimension_list TOKEN_SEMICOLON

template_reference    : TOKEN_NAME template_reference_name

template_reference_name : TOKEN_NAME | TOKEN_SEMICOLON

following_primitive_type : TOKEN_NAME | TOKEN_SEMICOLON

primitive_type        : TOKEN_WORD
                      | TOKEN_DWORD
                      | TOKEN_FLOAT
                      | TOKEN_DOUBLE
                      | TOKEN_CHAR
                      | TOKEN_UCHAR
                      | TOKEN_SWORD
                      | TOKEN_SDWORD
                      | TOKEN_LPSTR
                      | TOKEN_UNICODE
                      | TOKEN_CSTRING

